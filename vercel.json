{\n  "version": 2,\n  "builds": [\n    {\n      "src": "api/**/*.js",\n      "use": "@vercel/node"\n    }\n  ],\n  "routes": [\n    {\n      "src": "/api/(.*)",\n      "dest": "/api/$1"\n    },\n    {\n      "src": "/(.*)",\n      "dest": "/$1"\n    }\n  ],\n  "headers": [\n    {\n      "source": "/api/(.*)",\n      "headers": [\n        {\n          "key": "Cache-Control",\n          "value": "s-maxage=3600, stale-while-revalidate=86400"\n        }\n      ]\n    }\n  ]\n}